#include <iostream>
using namespace std;

int main()
{
	int choice;

	cout << "Please enter your choice for this menu:";
	cin >> choice;

	switch (choice)
	{
	case 1: cout << "Adult Gym \n";
				break;
	case 2: cout << "Kids Gym \n";
				break;
	case 3: cout << "Senior Cetizen Gym \n";
				break;
	case 4: cout << "Exit the menu\n";

	default: cout << "Please enter number between 1-3.\n";
	}

	cout << "Thank you for using our Gym Services.\n";
    return 0;
}
